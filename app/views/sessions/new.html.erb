<%= form_tag session_path do %>
	<div class="field">
		<%= label_tag :email %><br />
		<%= text_field_tag :email %>
	</div>
	<div class="field">
		<%= label_tag :password, 'Senha' %><br />
		<%= password_field_tag :password %>
	</div>
	<div class="actions">
		<%= submit_tag 'Logar' %>
	</div>
<% end %>